{
  "name": "@seungchan-dev/ui",
  "version": "0.0.0",
  "private": true,
  "description": "design system of seungchan-dev",
  "keywords": [
    "react",
    "design-system",
    "vanilla-extract"
  ],
  "author": {
    "name": "sxungchxn",
    "email": "uvzone@naver.com"
  },
  "type": "module",
  "main": "dist/seungchan-dev-ui.cjs",
  "module": "dist/seungchan-dev-ui.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/seungchan-dev-ui.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/seungchan-dev-ui.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "vite build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@seungchan-dev/icons": "workspace:*",
    "@storybook/addon-docs": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/builder-vite": "^7.6.10",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "^7.6.6",
    "@storybook/test": "^7.6.9",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vanilla-extract/private": "^1.0.3",
    "@vanilla-extract/recipes": "^0.5.1",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^3.9.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "clsx": "^2.1.0",
    "deepmerge": "^4.3.1",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^24.0.0",
    "rollup-plugin-visualizer": "^5.11.0",
    "storybook": "^7.6.6",
    "storybook-theme-css-vars": "^0.0.12",
    "tsconfig": "workspace:*",
    "typescript": "5.4.5",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.7.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "framer-motion": "^11.0.3"
  }
}
